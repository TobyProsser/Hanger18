{
  "expo": {
    "name": "ClimberLB",
    "slug": "ClimberLB",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "6",
      "infoPlist": {
      "NSPhotoLibraryUsageDescription": "We need access to your photo library to select the profile image.",
      "NSCameraUsageDescription": "We need access to your camera to take photos.",
      "NSLocationWhenInUseUsageDescription": "Allow ClimberLB to ensure you are at a climbing gym when submitting a climb.",
      "UIBackgroundModes": ["fetch"]
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.fathomcreative.hanger18"
    },
    "android": {
      "versionCode": 2,
      "permissions": ["Allow app to ensure you are at a climbing gym when submitting a climb."],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fathomcreative.hanger18"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      "@react-native-firebase/app",
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fe832493-ae7b-4a57-99ee-276ac4a41492"
      }
    }
  }
}
